local ScreenGui = Instance.new("ScreenGui")
ScreenGui.Parent = game.CoreGui
ScreenGui.Name = "RyboweUI"

local Frame = Instance.new("Frame")
Frame.Size = UDim2.new(0, 300, 0, 250)
Frame.Position = UDim2.new(0.5, -150, 0.5, -125)
Frame.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
Frame.BorderSizePixel = 0
Frame.Parent = ScreenGui

local Title = Instance.new("TextLabel")
Title.Size = UDim2.new(1, 0, 0, 30)
Title.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
Title.TextColor3 = Color3.fromRGB(255, 255, 255)
Title.Text = "Rybowe Hub - Initializing... "
Title.Font = Enum.Font.SourceSansBold
Title.TextSize = 18
Title.Parent = Frame

local ScrollingFrame = Instance.new("ScrollingFrame")
ScrollingFrame.Size = UDim2.new(1, -10, 1, -40)
ScrollingFrame.Position = UDim2.new(0, 5, 0, 35)
ScrollingFrame.CanvasSize = UDim2.new(0, 0, 0, 0) 
ScrollingFrame.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
ScrollingFrame.ScrollBarThickness = 5
ScrollingFrame.Parent = Frame

local UIListLayout = Instance.new("UIListLayout")
UIListLayout.Parent = ScrollingFrame
UIListLayout.SortOrder = Enum.SortOrder.LayoutOrder
UIListLayout.Padding = UDim.new(0, 3)


local function addLogMessage(text, delayTime)
    task.wait(delayTime or 0) 
    local Log = Instance.new("TextLabel")
    Log.Size = UDim2.new(1, -10, 0, 20)
    Log.BackgroundTransparency = 1
    Log.TextColor3 = Color3.fromRGB(200, 200, 200)
    Log.Text = text
    Log.TextXAlignment = Enum.TextXAlignment.Left
    Log.Font = Enum.Font.SourceSans
    Log.TextSize = 14
    Log.Parent = ScrollingFrame
    Log.LayoutOrder = #ScrollingFrame:GetChildren() 


    task.wait(0.1)
    ScrollingFrame.CanvasSize = UDim2.new(0, 0, 0, UIListLayout.AbsoluteContentSize.Y)
end


addLogMessage("üîÑ Initializing Rybowe Hub Fix...", 0.5)
addLogMessage("üîç Checking environment...", 1)
addLogMessage("‚úÖ Executor detected!", 1.5)
addLogMessage("üîÑ Loading core files...", 2)
Title.Text = "Rybowe Hub - Loading!"
addLogMessage("üìÇ Verifying folder structure...", 2.5)
Title.Text = "Rybowe Hub - Verifying!"



local RyboweFolder = "Rybowe"
local folderPath = RyboweFolder .. "/Key System"

local function ensureFolderExists(folder)
    if isfolder(folder) then
        addLogMessage("‚úÖ Folder exists: " .. folder, 3)
    else
        makefolder(folder)
        addLogMessage("üìÇ Folder created: " .. folder, 3.5)
    end
end

ensureFolderExists(RyboweFolder)
ensureFolderExists(folderPath)


addLogMessage("‚öôÔ∏è Applying security patches...", 4)
Title.Text = "Rybowe Hub - Almost There!"
addLogMessage("üìú Checking Errors...", 4.5)
addLogMessage("‚úÖ Rybowe Hub Fixed Successfully!", 5)
Title.Text = "Rybowe Hub - Fixed!"
